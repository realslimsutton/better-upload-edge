{
  "name": "better-upload-edge",
  "description": "Simple and easy file uploads for React",
  "version": "0.0.5",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "tsc",
    "test": "vitest"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./server": {
      "import": "./dist/server/index.mjs",
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./server/helpers": {
      "import": "./dist/server-helpers/index.mjs",
      "types": "./dist/server-helpers/index.d.ts",
      "default": "./dist/server-helpers/index.js"
    },
    "./client": {
      "import": "./dist/client/index.mjs",
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    },
    "./client/helpers": {
      "import": "./dist/client-helpers/index.mjs",
      "types": "./dist/client-helpers/index.d.ts",
      "default": "./dist/client-helpers/index.js"
    },
    "./server/internal": {
      "import": "./dist/server/types/internal.mjs",
      "types": "./dist/server/types/internal.d.ts",
      "default": "./dist/server/types/internal.js"
    },
    "./client/internal": {
      "import": "./dist/client/types/internal.mjs",
      "types": "./dist/client/types/internal.d.ts",
      "default": "./dist/client/types/internal.js"
    }
  },
  "dependencies": {
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "aws4fetch": "^1.0.20",
    "zod": "^4.0.5"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "*",
    "react": "*"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-s3": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "@types/node": "^22.7.0",
    "@types/react": "^18",
    "react": "^18",
    "tsdown": "^0.12.9",
    "typescript": "^5.6.3",
    "vitest": "^4.0.3"
  },
  "keywords": [
    "react",
    "nextjs",
    "upload",
    "easy uploads",
    "simple",
    "files",
    "file upload",
    "multipart upload"
  ],
  "license": "MIT",
  "homepage": "https://better-upload.com",
  "bugs": {
    "url": "https://github.com/Nic13Gamer/better-upload/issues"
  },
  "author": "Nicholas",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nic13Gamer/better-upload.git"
  }
}
